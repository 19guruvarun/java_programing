class ElseIf_exp 
{
	public static void main(String[] args) 
	{
		int a=1,b=2,c=3,d=4;
		if(a>b && a>c && a>d)
			System.out.println(a+" is largest");
		else if(b>a && b>c && b>d)
			System.out.println(b+" is largest");
		else if(c>b && c>a && c>d)
			System.out.println(c+" is largest");
		else
			System.out.println(d+" is largest");
	}
}
